(class other_class_2
  (field int a 20)
  (method void foo ((int s)) (print (+ s a)))
)

(class other_class
  (field int a 10)
  (method void foo ((int q) (int r))
   (begin
     (call (new other_class_2) foo 10)
     (print (+ a (+ q r)))
   )
  )
)

(class main
  (field other_class other null)
  (method void main () 
    (begin 
      (set other (new other_class))
      (call other foo 5 6)
    )
  )
)

