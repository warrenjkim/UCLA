#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <string.h>

#define BITSET_WIDTH = 32;

#define REGISTER_WIDTH 5

#define FUNCT3_WIDTH 3
#define FUNCT7_WIDTH 7

#define OPCODE_WIDTH 7

#define I_TYPE_IMM_WIDTH 12
#define B_TYPE_IMM_WIDTH 12
#define J_TYPE_IMM_WIDTH 20

struct bitset {
  size_t width;
  unsigned char *data;
};

void init_bitset(bitset *set, size_t width) {
  set->width = width;
  
}