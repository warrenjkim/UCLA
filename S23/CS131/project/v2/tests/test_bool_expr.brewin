(class main
 (field bool t true)
 (field bool f false)
 (method void main ()
   (begin
     (print (| true false))
     (print (& true false))
     (print (! true))
     (print (| t f))
     (print (& t f))
     (print (! t))
     (print (| (== t true) false))
     (print (& (== t true) false))
     (print (! (== t true)))
   )
 )
)
