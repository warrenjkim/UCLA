(class main
  (method int value_or_zero ((int q)) 
     (begin
       (if (< q 0)
         (print "q is less than zero")
         (return q) # else case
       )
     )
   )
  (method void main ()
    (begin
      (print (call me value_or_zero 10))  # prints 10
      (print (call me value_or_zero -10)) # prints 0
    )
  )
)
